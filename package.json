{
  "name": "import-adjutor",
  "version": "0.0.0-dev",
  "description": "Parse ecmascript modules and collect names of export vars, functions, etc.",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.ts",
  "bin": "bin.js",
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "npm run mocha -- --no-timeouts src/*.spec.ts",
    "test:cov": "c8 --reporter text --exclude \"**/*.spec.ts\" npm run test:r",
    "test:w": "npm run mocha -- --no-timeouts --watch-files **/*.ts --watch src/**/*.spec.ts",
    "test:d": "node --inspect -r ts-node/register/transpile-only node_modules/mocha/bin/_mocha --no-timeouts --watch-extensions ts --watch src/**/*.spec.ts",
    "test:brk": "node --inspect-brk -r ts-node/register/transpile-only node_modules/mocha/bin/_mocha --no-timeouts src/**/*.spec.ts",
    "mocha": "node -r ts-node/register/transpile-only node_modules/mocha/bin/_mocha",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:fix": "npm run tslint:fix || true && npm run eslint:fix",
    "tslint:fix": "tslint -p tsconfig.json --fix",
    "build": "sh Taskfile build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/import-adjutor.git"
  },
  "keywords": [
    "esm",
    "typescript",
    "parser",
    "es2015",
    "exports"
  ],
  "license": "MIT",
  "author": "2020",
  "bugs": {
    "url": "https://github.com/unlight/import-adjutor/issues"
  },
  "homepage": "https://github.com/unlight/import-adjutor#readme",
  "dependencies": {
    "get-stdin": "^8.0.0",
    "globrex": "^0.1.2",
    "picomatch": "^2.2.2",
    "ts-morph": "^7.1.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular/core": "^10.0.3",
    "@angular/material": "^10.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@types/fs-readfile-promise": "^3.0.0",
    "@types/globrex": "^0.1.0",
    "@types/lodash.flatten": "^4.4.6",
    "@types/micromatch": "^4.0.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.22",
    "@types/picomatch": "^2.2.1",
    "@types/react": "^16.9.42",
    "@typescript-eslint/parser": "^3.6.0",
    "angular2-calendar": "^0.3.7",
    "c8": "^7.2.1",
    "common-tags": "^1.8.0",
    "eslint": "^7.4.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-unicorn": "^20.1.0",
    "fs-readfile-promise": "^3.0.1",
    "gulp-tslint": "^8.1.4",
    "hover": "^3.0.0-alpha.6",
    "lodash.flatten": "^4.4.0",
    "material-design-iconic-font": "^2.2.0",
    "mocha": "^8.0.1",
    "preact": "10.4.5",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-native": "^0.63.0",
    "rxjs": "^6.6.0",
    "semantic-release": "^17.1.1",
    "ts-node": "^8.10.2",
    "type-zoo": "3",
    "typescript": "^3.9.6",
    "watchexec-bin": "^1.0.0"
  }
}
